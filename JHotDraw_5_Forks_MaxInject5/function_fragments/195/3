    public synchronized SnmpInformRequest removeRequest(long reqid) {
        int max= size();
        for (int i = 0 ; i < max ; i++) {
            SnmpInformRequest reqc = getRequestAt(i) ;
            if (reqid == reqc.getRequestId()) {
              /* Comment */    removeElementAt(i) ;
                return reqc ;
            }
        }
        return null ;
    }
