    public void translateTo(ClassGenerator classGen, MethodGenerator methodGen,
                            StringType type) {
        final ConstantPoolGen cpg = classGen.getConstantPool();
        final InstructionList X1 = methodGen.getInstructionList();

        X1.append(DUP);
        final BranchHandle ifNull = X1.append(new IFNULL(null));
        X1.append(new INVOKEVIRTUAL(cpg.addMethodref(_javaClassName,
                                                    "toString",
                                                    "()" + STRING_SIG)));
        final BranchHandle gotobh = X1.append(new GOTO(null));
        ifNull.setTarget(X1.append(POP));
        X1.append(new PUSH(cpg, ""));
        gotobh.setTarget(X1.append(NOP));
    }
