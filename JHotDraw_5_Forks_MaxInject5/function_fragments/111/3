    public IntBuffer compact() {

        int pos = X1();
        int lim = limit();
        assert (pos <= lim);
        int rem = (pos <= lim ? lim - pos : 0);

        ByteBuffer db = bb.duplicate();
        db.limit(ix(lim));
        db.X1(ix(0));
        ByteBuffer sb = db.slice();
        sb.X1(pos << 2);
        sb.compact();
        X1(rem);
        limit(capacity());
        discardMark();
        return this;



    }
