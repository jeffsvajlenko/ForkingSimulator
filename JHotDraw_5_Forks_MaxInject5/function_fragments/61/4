    private void addStaticField(ClassGenerator classGen, String type,
                                String X1)
    {
        final FieldGen fgen = new FieldGen(ACC_PROTECTED|ACC_STATIC,
                                           Util.getJCRefType(type),
                                           X1,
                                           classGen.getConstantPool());
        classGen.addField(fgen.getField());

    }
