        public void changedUpdate(DocumentEvent X1) {
            if (getUpdatePolicy() == NEVER_UPDATE ||
                    (getUpdatePolicy() == UPDATE_WHEN_ON_EDT &&
                    !SwingUtilities.isEventDispatchThread())) {
                return;
            }
            if(X1 instanceof AbstractDocument.UndoRedoDocumentEvent) {
                setDot(X1.getOffset() + X1.getLength());
            }
        }
