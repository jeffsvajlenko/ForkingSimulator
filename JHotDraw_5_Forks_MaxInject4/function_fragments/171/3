    public void endConditional(Augmentations X1) throws XNIException {

        // set state
        fInDTDIgnore = false;

        // call handlers
        if(fDTDGrammar != null)
            fDTDGrammar.endConditional(X1);
        if (fDTDHandler != null) {
            fDTDHandler.endConditional(X1);
        }

    } // endConditional()
