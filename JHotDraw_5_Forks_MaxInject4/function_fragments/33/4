  public void insert_wstring (String value) throws X1.omg.DynamicAny.DynAnyPackage.TypeMismatch, X1.omg.DynamicAny.DynAnyPackage.InvalidValue
  {
      X1.omg.CORBA.portable.ServantObject $so = _servant_preinvoke ("insert_wstring", _opsClass);
      DynAnyOperations  $self = (DynAnyOperations) $so.servant;

      try {
         $self.insert_wstring (value);
      } finally {
          _servant_postinvoke ($so);
      }
  } // insert_wstring
