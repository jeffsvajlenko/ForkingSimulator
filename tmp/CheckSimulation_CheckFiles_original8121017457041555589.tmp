        X = 0;
    } // endDTD()

    // set the source of this handler
    public X X(X X)
    {
        X = X;
    }

    // return the source from which this handler derives its events
    public X X()
    {
        return X;
    }

    //
    // XMLDTDContentModelHandler methods
    //

