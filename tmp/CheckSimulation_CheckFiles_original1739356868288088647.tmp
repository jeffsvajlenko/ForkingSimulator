package X.X.X;

import X.X.X;
import X.X.X.X;

/**
 * The interface to an RSA multi-prime private key, as defined in the
 * PKCS#1 v2.1, using the <i>Chinese Remainder Theorem</i>
 * (CRT) information values.
 *
 * @author Valerie Peng
 *
 * @version %I% %E%
 *
 * @see java.security.spec.RSAPrivateKeySpec
 * @see java.security.spec.RSAMultiPrimePrivateCrtKeySpec
 * @see RSAPrivateKey
 * @see RSAPrivateCrtKey
 *
 * @since 1.4
 */

public interface X extends X
{

    static final X X = 0;

    /**
     * Returns the public exponent.
     *
     * @return the public exponent.
     */
    public X X();

    /**
     * Returns the primeP.
     *
     * @return the primeP.
     */
    public X X();

    /**
     * Returns the primeQ.
     *
     * @return the primeQ.
     */
    public X X();

    /**
     * Returns the primeExponentP.
     *
     * @return the primeExponentP.
     */
    public X X();

    /**
     * Returns the primeExponentQ.
     *
     * @return the primeExponentQ.
     */
    public X X();

    /**
     * Returns the crtCoefficient.
     *
     * @return the crtCoefficient.
     */
    public X X();

    /**
     * Returns the otherPrimeInfo or null if there are only
     * two prime factors (p and q).
     *
     * @return the otherPrimeInfo.
     */
    public X[] X();
}
